[credential]
  helper = osxkeychain
[core]
  editor = vim -c start
  excludesfile = ~/.gitignore_global
[alias]
# add
  aa = add .
  ap = add --patch

# commit
  cm = commit -a

# log
  lg = log --graph --date=short

# push
  p = push                         # push
